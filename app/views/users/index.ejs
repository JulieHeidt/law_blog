<div class="navBlocker"></div>

<section class="usersIndex">
  <div class="container">
    <div class="row">
      <div class="col-sm-12 text-center">
        <% for ( var i = 0; i < users.length; i++ )  { %>
          <ul class="ulInline">
            <li><h2><%= users[ i ].firstName %> <%= users[ i ].lastName %></h2></li>
            <li><h3><%= users[ i ].email %></h3></li>
          </ul>
          <hr />
        <% } %>
      </div>
    </div>
  </div>
</section>
